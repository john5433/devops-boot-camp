       agent {label  "agentfarm" }
       stages {
              stage('First-stage') {
                     steps {
                            echo 'This is First stage'
                     }
              }
             stage('Second-Stage') {
                    steps {
                           echo 'This is Second-stage'
                    }
              }
             stage('Third-Stage') {
                    steps {
                           echo 'This is Third-stage'
                    }
              }
        }
   }

